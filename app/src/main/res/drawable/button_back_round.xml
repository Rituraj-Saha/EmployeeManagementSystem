<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    >
    <item>
        <shape
            android:shape="rectangle"
            >
            <solid
                android:color="#20000000"/>
            <padding
                android:top="5dp"
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"/>
            <corners
                android:radius="25dp"/>
        </shape>
    </item>
    <item>
        <shape>
        <gradient
            android:startColor="#FB7403"
            android:endColor="#F0A904"
            />
        <corners
            android:radius="25dp"/>
        </shape>
    </item>


</layer-list>